{
  "name": "ember-sortable",
  "version": "4.0.3",
  "description": "Sortable UI primitives for Ember.",
  "keywords": [
    "ember-addon",
    "sortable"
  ],
  "repository": "https://github.com/adopted-ember-addons/ember-sortable",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.1",
    "@embroider/macros": "^1.9.0",
    "ember-cli-babel": "^7.26.11",
    "ember-auto-import": "^2.4.0",
    "ember-modifier": "^3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "https://adopted-ember-addons.github.io/ember-sortable/demo/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "extends": "../package.json"
  }
}
